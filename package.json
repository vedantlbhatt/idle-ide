{
  "name": "idle-ide",
  "displayName": "Idle IDE",
  "description": "Stay engaged while your AI agent works - scroll TikTok directly in your IDE",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:idle-ide.openTikTok"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "idle-ide.openTikTok",
        "title": "Open TikTok Panel",
        "category": "Idle IDE"
      },
      {
        "command": "idle-ide.closeTikTok",
        "title": "Close TikTok Panel",
        "category": "Idle IDE"
      },
      {
        "command": "idle-ide.toggleTikTok",
        "title": "Toggle TikTok Panel",
        "category": "Idle IDE"
      }
    ],
    "keybindings": [
      {
        "command": "idle-ide.toggleTikTok",
        "key": "cmd+shift+t",
        "mac": "cmd+shift+t"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "idle-ide.tiktokView",
          "name": "TikTok",
          "when": "idle-ide.tiktokPanelVisible"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}


