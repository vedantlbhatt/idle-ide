{
  "name": "tiktok-scroller",
  "displayName": "TikTok Scroller",
  "description": "Scroll TikTok in VS Code sidebar while coding",
  "version": "1.0.0",
  "publisher": "idle-ide",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tiktok.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tiktok.open",
        "title": "Open TikTok",
        "category": "TikTok"
      }
    ],
    "keybindings": [
      {
        "command": "tiktok.open",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "20.x",
    "typescript": "^5.3.3"
  }
}
